{
    "name": "csv-image/user-discounts",
    "description": "A reusable Laravel package for user-level discounts with deterministic stacking",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "CSV Image Team",
            "email": "team@csv-image.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "CsvImage\\UserDiscounts\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CsvImage\\UserDiscounts\\UserDiscountsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
